{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://moveandcleanweb-production.up.railway.app/api/:path*" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
